{
  "version": "0.10.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "unpkg": "build/embed.js",
  "files": [
    "dist",
    "build"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "browserify": "NODE_ENV=production browserify embed.js -s traversal -o build/embed.js -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] --plugins [ @babel/plugin-transform-runtime ] ]",
    "minify": "minify build/embed.js > build/embed.min.js",
    "embed": "npm run browserify && npm run minify",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "fix": "tsdx lint --fix",
    "prepare": "tsdx build && npm run embed"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-redux": ">=7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@doctorlink/traversal-react-redux",
  "author": "DoctorLink Team <ben.redmond-benham@doctorlink.com>",
  "module": "dist/ts-speed.esm.js",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "husky": "^4.2.1",
    "minify": "^5.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/lodash.merge": "^4.6.6",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.2",
    "framer-motion": "^1.9.0",
    "lodash.merge": "^4.6.2",
    "normalizr": "^3.5.0",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.1.2",
    "react-visibility-sensor": "^5.1.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.0"
  }
}
