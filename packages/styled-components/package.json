{
  "name": "@doctorlink/styled-components",
  "version": "0.8.2",
  "description": "Doctorlink styled components",
  "keywords": [
    "styled-components"
  ],
  "author": "DoctorLink Team <ben.redmond-benham@doctorlink.com>",
  "homepage": "https://github.com/DoctorLink/npm#readme",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DoctorLink/npm.git"
  },
  "bugs": {
    "url": "https://github.com/DoctorLink/npm/issues"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "start": "run-p \"build:** -- --watch\"",
    "test": "tsc --noEmit && jest test"
  },
  "dependencies": {
    "@doctorlink/traversal-core": "^0.8.2",
    "@doctorlink/traversal-redux": "^0.8.2",
    "@testing-library/jest-dom": "^5.11.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-visibility-sensor": "^5.0.1",
    "@types/styled-components": "^5.1.2",
    "framer-motion": "2.6.13",
    "jest-styled-components": "^7.0.2",
    "lodash.merge": "^4.6.2",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-visibility-sensor": "^5.1.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  }
}
